@model IEnumerable<TravelAgency.ViewModel.BookingModel>
@{
    ViewBag.Title = "ListBookingStatusFalse";
}



<!-- start: Content -->
<div id="content">
    <div class="panel box-shadow-none content-header">
        <div class="panel-body">
            <div class="col-md-12">
                <h3 class="animated fadeInLeft">Data Tables</h3>
                <p class="animated fadeInDown">
                    @Html.ActionLink("All Booking", "ListBooking", "Booking") <span class="fa-angle-right fa"></span>@Html.ActionLink("Waiting Confirmation", "ListBookingStatusFalse", "Booking") <span class="fa-angle-right fa"></span> @Html.ActionLink("Edit Reservation", "Index", "ReservationTs")
                </p>
            </div>
        </div>
    </div>
    <div class="col-md-12 top-20 padding-0">
        <div class="col-md-12">
            <div class="panel">
                <div class="panel-heading"><h3>Booking Tables</h3></div>
                <div class="panel-body">
                    <div class="responsive-table">
                        <table id="datatables-example" class="table table-striped table-bordered" width="100%" cellspacing="0">
                            <thead>
                                <tr>
                                    <th>@Html.DisplayNameFor(model => model.customername)</th>
                                    <th>@Html.DisplayNameFor(model => model.customersurname)</th>
                                    <th>@Html.DisplayNameFor(model => model.reservationnumber)</th>
                                    <th>@Html.DisplayNameFor(model => model.status)</th>

                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>

                                        <td>@Html.DisplayFor(modelItem => item.customername) </td>

                                        <td>@Html.DisplayFor(modelItem => item.customersurname) </td>

                                        <td>@Html.DisplayFor(modelItem => item.reservationnumber)</td>

                                        <td>@Html.DisplayFor(modelItem => item.status) </td>

                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- end: content -->

